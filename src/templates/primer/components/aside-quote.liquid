      </pattern>
      </defs>
      <rect width="100%" height="100%" fill="url(#:Rb6:)"></rect></svg>


    </div>



    <div class="mx-auto px-4 sm:px-6 md:max-w-2xl md:px-4 lg:px-2 relative">
      <figure>
        <div class="flex text-slate-900 sm:justify-center">
          <div class="flex gap-1">

