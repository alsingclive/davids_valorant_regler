{% if data.title != blank %}
{% title data.title | append: " | TOGAFY" %}
{% else %}
{% title "TOGAFY" %}
{% endif %}

{% html, lang: "da", class: "scroll-smooth" %}
{% body, class: "selection:bg-primary/25 overflow-x-hidden" %}

{% meta 'viewport', content: 'width=device-width, initial-scale=1, user-scalable=1' %}

{% link "https://fonts.googleapis.com/icon?family=Material+Icons", rel: "stylesheet" %}
{% link "https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,200,0,0", rel:
"stylesheet" %}
{% link "/theme.css", rel: "stylesheet" %}
{% script "/main.js", location: "footer", async %}

{% # render "/templates/layout/navigation.liquid", navigation: data.navigation %}

<div id="main" class="w-full min-h-screen my-0 flex flex-col {% if data.url == " /protocol" %}bg-zinc-900{% elsif
  data.url=="/protocol/demo" %}bg-zinc-900{% endif %}">
  {% block content %} Loading content... {% endblock %}
</div>